@charset "utf-8";

@import url("font.css");

/* common reset */
*{margin: 0; padding: 0;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,form,fieldset,a,p,button,header,footer,table,caption,thead,tr,th,tbody,td,span,legend{margin: 0; padding: 0;}
body,input,select,textarea,header,footer,button{color: #444; font-family: '나눔고딕','Nanum Gothic', '돋움',Dotum,Arial,sans-serif; font-size: 12px; font-weight: 600;}
/* 글씨 크기 지정*/
body{background: #fff;-wekit-text-size-adjust:none;}

img,fieldset{border:0 none;}
img,input,select,textarea{vertical-align: middle;}
input{border: 1px solid #c9c9c9; padding-left: 5px; height: 26px;} /* border 색깔 넣어주기 */
select{border: 1px solid #c9c9c9; padding: 5px; height: 30px; line-height: 24px;}
ul,ol,li{list-style: none;}
button{background: none; border: none;}

a{color: inherit; text-decoration: none;}
a:link,a:hover,a:active{text-decoration: none; color: inherit;}

@media (max-width: 1220px) {
    #wrap {
        width: 1200px;
    }
}
/* text-align */
.text-center{
    text-align: center !important;
}
.text-right{
    text-align: right !important;
}
.text-left{
    text-align: left !important;
}
/* //text-align */
.font12 {
    font-size: 12px;
}
.popup hr, .contents hr {
    margin-top: 40px;
    border: 1px solid #303030;
}
.box_search hr {
    margin: 10px 0;
    border-top: 1px solid #bdcfdc !important;
    width: 100%;
    border: none;
}
table caption{
    position: relative;
    width: 1px;
    height: 1px;
    padding: 0;
    overflow: hidden;
    clip: rect(0,0,0,0);
    border: 0;
    text-indent: -50000px;
}
.sr-only{
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0,0,0,0);
    border: 0;
}
.skip-navigation {
    position: relative;
}
.skip-navigation a {
    display: block;
    height: 1px;
    width: 1px;
    margin-bottom: -1px;
    overflow: hidden;
    text-align: center;
    color: #000;
    white-space: nowrap;
}
.skip-navigation a:focus, .skip-navigation a:active {
    display: inline-block;
    padding: 2px 5px;
    height: 15px;
    width: 80px;
    color: #fff;
    background: #d5001e;
    border: 1px solid #000000;
    position: absolute;
}
/* header */
