body {line-height: 1;
	font-family: 'NanumBarunGothic', '나눔바른고딕', 'NanumGothic', '나눔고딕','NanumGothicWeb', '나눔고딕웹', Dotum,AppleGothic,Arial Unicode MS,sans-serif;}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}

div {position: relative;}
a {color:#ffffff; text-decoration:none;}
img {border:0;}
button, input, select {border:none; outline: none; font-family: 'NanumBarunGothic', '나눔바른고딕', 'NanumGothic', '나눔고딕','NanumGothicWeb', '나눔고딕웹', Dotum,AppleGothic,Arial Unicode MS,sans-serif;}
